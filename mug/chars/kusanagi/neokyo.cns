jump.fwd = 3.15,-10.4      ;Jump forward Speed (x, y)
runjump.fwd = 4,-9.7      ;.
airjump.neu = 0,-9.7      ;.
airjump.back = -2.55,-9.7 ;Air jump speeds (opt)
airjump.fwd = 2.5,-9.7    ;.

[Movement]
