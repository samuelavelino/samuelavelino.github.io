; Definition file for kfm
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Ryu"        ;Name of character
displayname = "Ryu" ;Name of character to display
versiondate = 02,20,2005    ;Version date of character
mugenversion = 04,14,2002   ;Version of M.U.G.E.N character works on
author = "SMEE"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference
pal.defaults = 1,2,3,4,5,6      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = ryunes.cmd   ;Command set
cns     = ryunes.cns   ;Constants
st      = normal.st   ;States
st1	= Special.st
st2	= ai.st
stcommon = common.cns ;Common states
sprite  = ryunes.sff   ;Sprite
anim    = ryunes.air   ;Animation
sound   = ryunes.snd   ;Sound
pal1    = pal01.act  ;Palettes (can have up to 12)
pal2    = pal02.act
pal3    = pal03.act
pal4    = pal04.act
pal5    = pal05.act
pal6    = pal06.act
pal7    = pal07.act
pal8    = pal08.act
pal9    = pal09.act
pal10   = pal10.act
pal11   = pal11.act
pal12   = pal12.act

[Arcade]
ending.storyboard = ending.def